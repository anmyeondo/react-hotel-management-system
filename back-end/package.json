{
  "name": "back-end",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "server": "nodemon server.js",
    "client": "cd ../front-end && yarn start",
    "front-end-update": "cd ../front-end && yarn",
    "update": "yarn && yarn front-end-update",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "fs": "^0.0.1-security",
    "http-errors": "~1.6.3",
    "ip": "^1.1.5",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.6",
    "socket.io": "^3.0.1"
  }
}
