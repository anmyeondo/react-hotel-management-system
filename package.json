{
    "name": "management",
    "version": "1.0.0",
    "scripts": {
        "client": "cd front-end && yarn start",
        "server": "nodemon server.js",
        "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
        "client-update": "cd front-end && yarn",
        "update": "yarn && yarn client-update"
    },
    "dependencies": {
        "axios": "^0.20.0",
        "body-parser": "^1.19.0",
        "express": "^4.17.1",
        "global": "^4.4.0",
        "mysql": "^2.18.1",
        "nodemon": "^2.0.4"
    },
    "devDependencies": {
        "concurrently": "^4.1.2",
        "http-proxy-middleware": "^1.0.5"
    }
}
